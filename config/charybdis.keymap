        };
        KAKKO3: KAKKO3 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp AT &kp AT &kp LEFT_ARROW>;
            label = "KAKKO3";
        };
        KAKKO4: KAKKO4 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp RIGHT_BRACE &kp PIPE &kp LEFT_ARROW>;
            label = "KAKKO4";
        };
        mail_FROM: mail_FROM {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LC(E) &kp LANG2 &kp LS(F) &kp LS(R) &kp LS(O) &kp LS(M) &kp SINGLE_QUOTE>;
            label = "MAIL_FROM";
        };

        OMOJI: OMOJI {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp F9 &kp F9 &kp F8 &kp ENTER>;
            label = "OMOJI";
        };

        OMOJI2: OMOJI2 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp F9 &kp F9 &kp F8 &kp F8 &kp ENTER>;
            label = "OMOJI2";
        };

        SHOMEI: SHOMEI {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LEFT_ALT &kp N9 &kp ENTER>;
            label = "SHOMEI";
        };
        
        DCLICK: DCLICK {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&mkp LCLK &mkp LCLK>;
            label = "DCLICK";
        };

        TCLICK: TCLICK {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&mkp LCLK &mkp LCLK &mkp LCLK>;
            label = "TCLICK";
        };

        WINUP: WINUP {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&sk LEFT_GUI &kp UP_ARROW>;
            label = "WINUP";
        };

        WINDOWN: WINDOWN {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&sk LEFT_GUI &kp DOWN>;
            label = "WINDOWN";
        };

        WINLEFT: WINLEFT {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&sk LEFT_GUI &kp LEFT>;
            label = "WINLEFT";
        };

        WINRIGHT: WINRIGHT {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&sk LEFT_GUI &kp RIGHT>;
            label = "WINRIGHT";
        };
        INYO: INYO {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LS(PERIOD) &kp LC(V)>;
            label = "INYO";
        };
        EXPLO: EXPLO {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LC(C) &kp LG(E) &kp F4 &kp LC(V) &kp ENTER>;
            label = "EXPLO";
        };
        midashi: midashi {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp INT_MUHENKAN &kp LS(SQT) &kp SPACE &kp INT_HENKAN>;
            label = "MIDASHI";
        };

        markdown: markdown {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp INT_MUHENKAN &kp LS(SQT) &kp LS(SQT) &kp INT_HENKAN &kp LEFT>;
            label = "MARKDOWN";
        };

    };






  combos {
    compatible = "zmk,combos";
    timeout-ms = <20>;
    require-prior-idle-ms = <250>;

    // Studio Unlock Combo - all three right thumb keys
    combo_studio_unlock {
      timeout-ms = <50>;
      key-positions = <53 54 55>;
      bindings = <&studio_unlock>;
    };

