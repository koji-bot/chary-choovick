    behaviors {
        TD_ctrlBS: TD_ctrlBS {
            compatible = "zmk,behavior-tap-dance";
            label = "TD_CTRLBS";
            #binding-cells = <0>;
            bindings =
                <&mt_B RIGHT_SHIFT BACKSPACE>,
                <&mt BACKSPACE RC(BACKSPACE)>;

            tapping-term-ms = <200>;
        };

        TD_ctrlZ: TD_ctrlZ {
            compatible = "zmk,behavior-tap-dance";
            label = "TD_CTRLZ";
            #binding-cells = <0>;
            bindings = <&mt_B LCTRL Z>, <&kp LC(Z)>;

            tapping-term-ms = <300>;
        };
        TD_DEL: TD_DEL {
            compatible = "zmk,behavior-tap-dance";
            label = "TD_DEL";
            #binding-cells = <0>;
            bindings = <&lt 4 TAB>, <&kp DEL>;
        };

        hm_OMOJI1: hm_OMOJI1 {
            compatible = "zmk,behavior-hold-tap";
            label = "HM_OMOJI1";
            bindings = <&kp>, <&OMOJI>;

            #binding-cells = <2>;
            tapping-term-ms = <100>;
        };

        hm_OMOJI2: hm_OMOJI2 {
            compatible = "zmk,behavior-hold-tap";
            label = "HM_OMOJI2";
            bindings = <&kp>, <&OMOJI2>;

            #binding-cells = <2>;
            tapping-term-ms = <100>;
        };
        TD_dot: TD_dot {
            compatible = "zmk,behavior-tap-dance";
            label = "TD_DOT";
            #binding-cells = <0>;
            bindings = <&kp PERIOD>, <&INYO>;
        };
        TD_midashi: TD_midashi {
            compatible = "zmk,behavior-tap-dance";
            label = "TD_MIDASHI";
            #binding-cells = <0>;
            bindings = <&kp COMMA>, <&midashi>;
        };

        TD_markdown: TD_markdown {
            compatible = "zmk,behavior-tap-dance";
            label = "TD_MARKDOWN";
            #binding-cells = <0>;
            bindings = <&kp C>, <&markdown>;
        };
